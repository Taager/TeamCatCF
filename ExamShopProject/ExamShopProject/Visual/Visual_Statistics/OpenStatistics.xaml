<Page x:Class="ExamShopProject.OpenStatistics"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExamShopProject"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="700"
      Title="OpenStatistics">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="73*"/>
            <RowDefinition Height="377*"/>
        </Grid.RowDefinitions>
        
        <DockPanel Grid.ColumnSpan="1" Grid.Row="1">
            <Frame x:Name="_statistics" />
        </DockPanel>
        <!--#region Buttons-->
        <Button Content="View" HorizontalAlignment="Left" VerticalAlignment="Center" Width="75" Margin="40,0,0,0"/>
        <!--#endregion-->
    </Grid>
</Page>
