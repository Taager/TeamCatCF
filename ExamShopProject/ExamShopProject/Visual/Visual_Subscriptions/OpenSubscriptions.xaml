<Page x:Class="ExamShopProject.OpenSubscriptions"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExamShopProject"
      mc:Ignorable="d" 
      d:DesignHeight="350" d:DesignWidth="600"
      Title="OpenSubscriptions">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="43*"/>
            <RowDefinition Height="307*"/>
        </Grid.RowDefinitions>
        <DockPanel Grid.RowSpan="2" Margin="0,43,0,-0.6" >
            <Frame x:Name="_viewSubscriptions" NavigationUIVisibility="Hidden"/>
        </DockPanel>
        <Button x:Name="btn_View" Content="view" HorizontalAlignment="Left" Margin="26,10,0,0" VerticalAlignment="Top" Width="75" Height="19" Click="btn_View_Click"/>
        <Button x:Name="btn_Create" Content="Create" HorizontalAlignment="Left" Margin="123,10,0,0" VerticalAlignment="Top" Width="75" Height="19" Click="btn_Create_Click"/>

    </Grid>
</Page>
